<%= provide(:title, "new card") %>
<h1> new card</h1>

<div class="row">
<div class="col-md-6 col-md-offset-3">
    <%= form_for(@flashcard) do |f| %>
    <%= f.label :front %>
    <%= f.text_field :front, class: 'form-control'%>
    
    <%= f.label :back %>
    <%= f.text_field :back, class: 'form-control'%>
    <%= f.submit "create card", class: "btn btn-default" %>
    <% end %>
  </div>
</div>
<div class="newflashspacer"></div>
<div class="backlink"><%= link_to 'Back', flashcards_path %></div>
