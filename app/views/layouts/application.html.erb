<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title)) %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
</head>
<body>
<div class="container-fluid"> 
    <div class="row-fluid"><%= render 'layouts/header' %></div>
    <div class="row-fluid"> <%= yield %> </div>
    <div class="debug_dump"><%= debug(params) if Rails.env.development? %></div>
    <div class="row-fluid"> <%= render 'layouts/footer' %> </div>
  </div>  
</body>
</html>
